config_version=5

[application]

config/name="PixelMan Adventures"
run/main_scene="res://scenes/Test Scene.tscn"
config/features=PackedStringArray("4.1", "C#", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"

[display]

window/stretch/aspect="keep_height"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="PixelMan Adventures"

[editor_plugins]

enabled=PackedStringArray("res://addons/codeandweb.texturepacker/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
